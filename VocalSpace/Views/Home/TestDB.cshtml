@model List<Selection>

<h2>徵選列表</h2>
        @foreach (var sel in Model)
        {
            <tr>
                <td>@sel.SelectionId</td>
                <td>@sel.Title</td>
        <td><img src="@sel.SelectionCoverPath"></td>
                <td>@sel.CreateTime</td>
            </tr>
        }